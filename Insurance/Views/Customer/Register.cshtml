@model IMS.Customer


 <form method="post">  
      @ViewData["a"]
      
    <h1>edit customer details</h1>
    
    <hr>

     <label><b> Customer Name </b> </label>
    <input type="text" asp-for="CustName" required class="form-control" />

    <label><b> Nominee Name </b> </label>
    <input  type="text" asp-for="Nominee"  required class="form-control" />

    <label><b>Date Of Birth</b> </label>
    <input  type="text" asp-for="Dob"  required class="form-control" />
    
    <label><b>Email</b></label>
    <input type="text" asp-for="Mail" required  class="form-control">
    
    <label><b>Mobile Number</b></label>
    <input type="text" asp-for="Mobileno"  required class="form-control">

    <label><b>Address</b></label>
