     <label><b>Gender</b></label>
    <input type="text" asp-for="Gender" class="form-control">

    <label><b>Pwd</b></label>
    <input type="text" asp-for="Pwd" required  class="form-control">


    <button type="submit"   class="btn btn-success" style="text-align:center"> Register</button> 

   
    
    </form>
    



